#. module: partner_communication_switzerland
#: model:email.template,body_html:partner_communication_switzerland.email_disaster_alert
msgid ""
"<div>\n"
"                    % set partner = object.partner_id\n"
"                    % set children = object.get_objects()."
"mapped('child_id')\n"
"                    % set disaster = children."
"mapped('child_disaster_impact_ids.disaster_id')[0]\n"
"                    % set firstname = children.get_list('firstname', 3, "
"'many children')\n"
"                </div>\n"
"                <p>\n"
"                    ${partner.salutation},\n"
"                    <br/>\n"
"                    <br/>\n"
"                    We want to share with you some news about ${children[0]."
"project_id.country_id.name}, where ${children.get('your sponsored child')} "
"${'lives' if len(children) == 1 else 'live'}.\n"
"                    <br/>\n"
"                    <br/>\n"
"                    <b>${disaster.name}</b>\n"
"                    <ul>\n"
"                        <li>Occurred on: ${disaster.disaster_date}</li>\n"
"                        <li>Disaster type: ${disaster.disaster_type}</li>\n"
"                        <li>(internal only: <a href=\"${disaster."
"access_link}\" target=\"blank\">details</a>)</li>\n"
"                    </ul>\n"
"                    % if disaster.icp_disaster_impact_ids:\n"
"                        <b>Project impacts:</b>\n"
"                        % for icp_impact in disaster."
"icp_disaster_impact_ids:\n"
"                            <ul>\n"
"                                <li>Impact description: ${icp_impact."
"disaster_impact_description}</li>\n"
"                                <li>Impact on ICP program: ${icp_impact."
"impact_on_icp_program}</li>\n"
"                                <li>Infrastructure: ${icp_impact."
"infrastructure}</li>\n"
"                                <li>Impact description: ${icp_impact."
"disaster_impact_description}</li>\n"
"                            </ul>\n"
"                        % endfor\n"
"                    % endif\n"
"                    <br/>\n"
"                    % for child in children:\n"
"                        % set impact = child.child_disaster_impact_ids[0]\n"
"                        <b>${child.name} - [${child.local_id}]</b>\n"
"                        <ul>\n"
"                            <li>Physical condition: ${impact."
"beneficiary_physical_condition}\n"
"                                % if impact."
"beneficiary_physical_condition_description:\n"
"                                    <br/>${impact."
"beneficiary_physical_condition_description}\n"
"                                % endif\n"
"                            </li>\n"
"                            % if impact.house_condition:\n"
"                                <li>House condition: ${impact."
"house_condition}</li>\n"
"                            % endif\n"
"                            % if impact.siblings_died_number:\n"
"                                <li>Siblings died: ${impact."
"siblings_died_number}</li>\n"
"                            % endif\n"
"                            % if impact.siblings_seriously_injured_number:\n"
"                                <li>Siblings seriously injured number: "
"${impact.siblings_seriously_injured_number}</li>\n"
"                            % endif\n"
"                            % if impact.caregivers_died_number:\n"
"                                <li>Caregivers died: ${impact."
"caregivers_died_number}</li>\n"
"                            % endif\n"
"                            % if impact."
"caregivers_seriously_injured_number:\n"
"                                <li>Caregivers seriously injured number: "
"${impact.caregivers_seriously_injured_number}</li>\n"
"                            % endif\n"
"                            % if impact.loss_ids:\n"
"                                <li> Personal effects lost:\n"
"                                    <ul>\n"
"                                        % for loss in impact.loss_ids:\n"
"                                            <li>${loss.name}</li>\n"
"                                        % endfor\n"
"                                    </ul>\n"
"                                </li>\n"
"                            % endif\n"
"                        </ul>\n"
"                    % endfor\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Yours sincerely\n"
"                </p>\n"
"            "
msgstr ""
"<div>\n"
"    % set partner = object.partner_id\n"
"    % set children = object.get_objects().mapped('child_id')\n"
"    % set disaster = children.mapped('child_disaster_impact_ids.disaster_id')"
"[0]\n"
"    % set firstname = children.get_list('firstname', 3, 'many children')\n"
"</div>\n"
"<p>\n"
"    ${partner.salutation},\n"
"    <br>\n"
"    <br>\n"
"    Nous aimerions vous faire part des nouvelles que nous avons reçues de "
"${children[0].project_id.country_id.name}, où ${children.get('your sponsored "
"child')} ${'habite' if len(children) == 1 else 'habitent'}.\n"
"    <br>\n"
"    <br>\n"
"    <b>${disaster.name}</b>\n"
"</p><ul>\n"
"    <li>Survenu le : ${disaster.disaster_date}</li>\n"
"    <li>Type : ${disaster.disaster_type}</li>\n"
