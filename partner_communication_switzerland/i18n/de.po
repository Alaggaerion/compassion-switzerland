"                            <br/>\n"
"                        % endfor\n"
"                        % for revision in child.household_id."
"revised_value_ids:\n"
"                            <li>${revision.name} : ${revision.old_value} -"
"&gt; ${revision.get_field_value()}</li>\n"
"                            <br/>\n"
"                        % endfor\n"
"                    </ul>\n"
"                </p>\n"
"            "
msgstr ""
