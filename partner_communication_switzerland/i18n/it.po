#. module: partner_communication_switzerland
#: model:email.template,body_html:partner_communication_switzerland.email_caregiver_change
msgid ""
"<div>\n"
"                    % set child = object.get_objects()\n"
"                    % set caregiver = child.household_id.primary_caregiver\n"
"                </div>\n"
"                <p>\n"
"                    ${object.partner_id.salutation},\n"
"                    <br/>\n"
"                    <br/>\n"
"                    We learned that the primary caregiver of ${child."
"firstname} is now ${child.get('his')}\n"
"                    % if 'Beneficiary' not in caregiver:\n"
"                        ${caregiver}\n"
"                    % else:\n"
"                        ${'sister' if 'Female' in caregiver else 'brother'}\n"
"                    % endif\n"
"                    .\n"
"                </p>\n"
"            "
msgstr ""
"<div>\n"
"                    % set child = object.get_objects()\n"
"                    % set gender = child.household_id.primary_caregiver_id."
"gender\n"
"                    % set caregiver = child.household_id.primary_caregiver\n"
"                </div>\n"
"                <p>\n"
"                    ${object.partner_id.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    Abbiamo appreso che la persona che si occupa di ${child."
"firstname} è ora \n"
"                    % if 'Beneficiary' not in caregiver:\n"
"                        ${'suo' if gender == 'M' else 'sua'} ${caregiver}\n"
"                    % else:\n"
"                        ${'sua sorella' if 'Female' in caregiver else 'suo "
"fratello'}\n"
"                    % endif\n"
"                    .\n"
"                </p>"

