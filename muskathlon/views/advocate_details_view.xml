<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_advocate_details_muskathlon" model="ir.ui.view">
        <field name="name">view.advocate.details.muskathlon</field>
        <field name="model">advocate.details</field>
        <field name="inherit_id" ref="partner_compassion.view_advocate_details_default"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group">
                <group string="Trip information">
                    <field name="birth_name"/>
                    <field name="passport_number"/>
                    <field name="passport_expiration_date"/>
                    <field name="emergency_name"/>
                    <field name="emergency_phone"/>
                    <field name="emergency_relation_type"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
